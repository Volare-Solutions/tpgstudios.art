{
	"name": "svelte-ecom",
	"version": "0.0.2",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"generate": "drizzle-kit generate:sqlite",
		"db:push": "drizzle-kit push:mysql --config=drizzle.config.ts",
		"db:studio": "drizzle-kit studio",
		"db:seed": "tsx seed.ts",
		"stripe:listen": "stripe listen --forward-to http://localhost:5173/api/stripe",
		"email:dev": "email dev --dir src/lib/emails"
	},
	"devDependencies": {
		"@sveltejs/adapter-vercel": "^4.0.5",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/react": "^18.2.51",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"autoprefixer": "^10.4.17",
		"drizzle-kit": "^0.20.14",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.33",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.2.9",
		"svelte-adapter-bun": "^0.5.1",
		"svelte-check": "^3.6.3",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3",
		"vite": "^5.0.12"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-drizzle": "1.0.0-beta.5",
		"@planetscale/database": "^1.14.0",
		"@radix-ui/react-dialog": "^1.0.5",
		"@react-email/components": "0.0.14",
		"@react-email/tailwind": "^0.0.14",
		"@vercel/analytics": "^1.1.2",
		"arctic": "^0.10.6",
		"bits-ui": "^0.9.9",
		"cloudinary": "^2.0.1",
		"clsx": "^2.1.0",
		"csv-parse": "^5.5.3",
		"dotenv": "^16.4.1",
		"drizzle-orm": "^0.29.3",
		"lucia": "3.0.0-beta.14",
		"lucide-svelte": "^0.294.0",
		"mysql2": "^3.9.1",
		"oslo": "^0.24.0",
		"pnpm": "^8.15.4",
		"react": "^18.2.0",
		"react-email": "2.0.0",
		"resend": "^3.1.0",
		"stripe": "^14.14.0",
		"svelte-cloudinary": "^1.2.1",
		"tailwind-merge": "^2.2.1",
		"tailwind-variants": "^0.1.20",
		"vaul": "^0.8.9",
		"vaul-svelte": "^0.1.1",
		"zod": "^3.22.4",
		"zod-form-data": "^2.0.2"
	}
}
